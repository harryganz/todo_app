{
  "name": "todo_app",
  "version": "0.0.0",
  "description": "A simple todo list application",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "./bin/www",
    "pretest": "npm install",
    "test": "node node_modules/karma/bin/karma start tests/karma.conf.js",
    "test-single": "node node_modules/karma/bin/karma start tests/karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm start & npm run update-webdriver",
    "protractor": "protractor tests/protractor.conf.js",
    "mocha": "mocha tests/backend/backendSpec.js -w",
    "mocha-single": "mocha tests/backend/backendSpec.js"
  },
  "author": "Harry Ganz <hganz@rsmas.miami.edu>",
  "license": "MIT",
  "devDependencies": {
    "karma": "~0.13.10",
    "jasmine": "~2.3.2",
    "karma-chrome-launcher": "~0.2.0",
    "karma-firefox-launcher": "~0.1.6",
    "jasmine-core": "~2.3.4",
    "karma-jasmine": "~0.3.6",
    "mocha": "~2.3.3",
    "supertest": "~1.1.0"
  },
  "dependencies": {
    "express": "~4.13.3",
    "express-negotiate": "0.0.5"
  }
}
